{
  "name": "zazu-homebrew",
  "version": "1.2.0",
  "description": "Zazu plugin to search for Homebrew formulae with braumeister.org",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "lint": "standard --verbose | snazzy",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielbayerlein/zazu-homebrew.git"
  },
  "keywords": [
    "zazu",
    "homebrew",
    "brew",
    "braumeister"
  ],
  "author": "Daniel Bayerlein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielbayerlein/zazu-homebrew/issues"
  },
  "homepage": "https://github.com/danielbayerlein/zazu-homebrew#readme",
  "devDependencies": {
    "husky": "^0.14.3",
    "jest": "^21.2.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "cache-conf": "^0.6.0",
    "scrape-it": "^3.3.3"
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/*.js"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
