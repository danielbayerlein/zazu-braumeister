{
  "name": "zazu-homebrew",
  "version": "1.0.0",
  "description": "Zazu plugin to search for Homebrew formulae with braumeister.org",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "lint": "eslint --format=node_modules/eslint-formatter-pretty .",
    "test": "npm run lint && jest",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielbayerlein/zazu-homebrew.git"
  },
  "keywords": [
    "zazu",
    "homebrew",
    "brew",
    "braumeister"
  ],
  "author": "Daniel Bayerlein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielbayerlein/zazu-homebrew/issues"
  },
  "homepage": "https://github.com/danielbayerlein/zazu-homebrew#readme",
  "devDependencies": {
    "eslint": "^3.16.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.13.1",
    "jest": "^18.1.0"
  },
  "dependencies": {
    "scrape-it": "^3.2.2"
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/*.js"
    ]
  }
}
