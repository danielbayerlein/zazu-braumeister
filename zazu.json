{
  "name": "zazu-homebrew",
  "version": "1.0.0",
  "description": "Search for Homebrew formulae with braumeister.org",
  "icon": "icon.png",
  "blocks": {
    "input": [
      {
        "id": "homebrew",
        "type": "PrefixScript",
        "prefix": "brew",
        "space": true,
        "args": "Required",
        "script": "src/index.js",
        "connections": ["Open"]
      }
    ],
    "output": [
      {
        "id": "Open",
        "type": "OpenInBrowser",
        "url": "{value}"
      }
    ]
  }
}
